{
    "name": "typescript-start-kit",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "git@github.com-PracticeEveryday:PracticeEveryday/typescript-start-kit.git",
    "license": "MIT",
    "bin": "./bin/index.js",
    "scripts": {
        "prepare": "[ ! -d .husky ] && husky install || echo \"Husky directory already exists\"",
        "test:unit": "NODE_ENV=test jest --config jest-unit.config.js --passWithNoTests",
        "test:e2e": "NODE_ENV=test jest --config jest-e2e.config.js --passWithNoTests",
        "lint-fix": "eslint '**/*.{ts,tsx}' --ext .ts,.tsx --fix",
        "build": "rm -rf dist && npx tsc --project tsconfig.json",
        "postinstall": "[ ! -d .husky ] && npx husky install || echo \"Husky directory already exists\"",
        "start:dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/bin/index.ts",
        "start:prod": "yarn run build && cross-env NODE_ENV=production node ./dist/src/bin/index.js"
    },
    "dependencies": {
        "@types/cors": "^2.8.13",
        "colors": "^1.4.0",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.10",
        "eslint": "^8.35.0",
        "express": "^4.18.2",
        "helmet": "^6.0.1",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.21",
        "typescript": "^5.3.2",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.10",
        "@types/morgan": "^1.9.4",
        "@types/node": "^18.14.6",
        "@types/supertest": "^2.0.16",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "dotenv": "^16.0.3",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1"
    }
}
